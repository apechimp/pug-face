{
  "manifest_version": 2,

  "name": "pug-face",
  "description": "Replaces peoples faces with faces of pugs.",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["extension.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "PUUUUUUUUUUUGS!!!"
  }
}
